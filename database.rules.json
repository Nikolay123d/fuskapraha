{
  "rules": {
    ".read": false,
    ".write": false,
    "roles": {
      "$uid": {
        ".read": "auth != null && (auth.uid === $uid || auth.token.email === 'urciknikolaj642@gmail.com')",
        ".write": "auth != null && auth.token.email === 'urciknikolaj642@gmail.com'"
      }
    },
    "bans": {
      "$uid": {
        ".read": "auth != null && (auth.uid === $uid || auth.token.email === 'urciknikolaj642@gmail.com')",
        ".write": "auth != null && (auth.token.email === 'urciknikolaj642@gmail.com' || root.child('roles/'+auth.uid+'/moderator').val() === true)"
      }
    },
    "messages": {
      ".read": true,
      "$city": {
        ".indexOn": [
          "ts",
          "at"
        ],
        "$msg": {
          ".write": "auth != null && !(root.child('bans/'+auth.uid+'/until').val() > now)",
          ".validate": "newData.hasChild('text') || newData.hasChild('img') || newData.hasChild('image') || newData.hasChild('nick') && (newData.child('ts').isNumber() || newData.child('at').isNumber()) && (newData.child('by').isString() || newData.child('from').isString())"
        }
      }
    },
    "chat": {
      ".read": true,
      "$city": {
        ".indexOn": [
          "ts",
          "at"
        ],
        "$msg": {
          ".write": "auth != null && !(root.child('bans/'+auth.uid+'/until').val() > now)",
          ".validate": "newData.hasChild('text') || newData.hasChild('img') || newData.hasChild('image') || newData.hasChild('nick') && (newData.child('ts').isNumber() || newData.child('at').isNumber()) && (newData.child('by').isString() || newData.child('from').isString())"
        }
      }
    },
    "rentMessages": {
      ".read": true,
      "$city": {
        ".indexOn": [
          "ts"
        ],
        "$msg": {
          ".write": "auth != null && !(root.child('bans/'+auth.uid+'/until').val() > now)",
          ".validate": "newData.hasChild('ts') && newData.child('ts').isNumber()"
        }
      }
    },
    "users": {
      "$uid": {
        ".read": "auth != null && (auth.uid === $uid || auth.token.email === 'urciknikolaj642@gmail.com')",
        ".write": "auth != null && (auth.uid === $uid || auth.token.email === 'urciknikolaj642@gmail.com')"
      }
    },
    "usersPublic": {
      "$uid": {
        ".read": true,
        ".write": "auth != null && auth.uid === $uid"
      }
    },
    "presence": {
      "$uid": {
        ".read": true,
        ".write": "auth != null && auth.uid === $uid"
      }
    },
    "privateMembers": {
      "$room": {
        "$uid": {
          ".read": "auth != null && root.child('privateMembers/'+$room+'/'+auth.uid).val() === true",
          ".write": "auth != null && !(root.child('bans/'+auth.uid+'/until').val() > now) && (auth.uid === $uid || root.child('privateMembers/'+$room+'/'+auth.uid).val() === true)",
          ".validate": "newData.isBoolean()"
        }
      }
    },
    "privateMessages": {
      "$room": {
        ".read": "auth != null && root.child('privateMembers/'+$room+'/'+auth.uid).val() === true && root.child('roomsBlocked/'+$room).val() !== true",
        ".write": "auth != null && root.child('privateMembers/'+$room+'/'+auth.uid).val() === true && root.child('roomsBlocked/'+$room).val() !== true && !(root.child('bans/'+auth.uid+'/until').val() > now)",
        ".indexOn": [
          "ts",
          "at"
        ]
      }
    },
    "inboxMeta": {
      "$uid": {
        ".read": "auth != null && auth.uid === $uid",
        "$room": {
          ".write": "auth != null && auth.uid === $uid",
          ".validate": "newData.hasChild('ts') && newData.child('ts').isNumber() || newData.hasChild('lastTs') && newData.child('lastTs').isNumber()"
        }
      }
    },
    "roomsBlocked": {
      "$room": {
        ".read": "auth != null && auth.token.email === 'urciknikolaj642@gmail.com'",
        ".write": "auth != null && auth.token.email === 'urciknikolaj642@gmail.com'",
        ".validate": "newData.isBoolean()"
      }
    },
    "friends": {
      "$uid": {
        ".read": "auth != null && auth.uid === $uid",
        "$friend": {
          ".write": "auth != null && auth.uid === $uid"
        }
      }
    },
    "friendRequests": {
      "$uid": {
        ".read": "auth != null && auth.uid === $uid",
        "$from": {
          ".write": "auth != null"
        }
      }
    },
    "notifications": {
      "$uid": {
        ".read": "auth != null && auth.uid === $uid",
        "$nid": {
          ".write": "auth != null",
          ".validate": "newData.hasChild('ts') && newData.child('ts').isNumber()"
        }
      }
    },
    "vacancies": {
      "$uid": {
        ".read": true,
        ".write": "auth != null && auth.uid === $uid",
        ".indexOn": [
          "ts",
          "city"
        ]
      }
    },
    "ratings": {
      "$uid": {
        ".read": true,
        "$by": {
          ".write": "auth != null && auth.uid === $by",
          ".validate": "newData.isNumber() && newData.val() >= 1 && newData.val() <= 5"
        }
      }
    },
    "reportsUsers": {
      "$uid": {
        ".read": "auth != null && (auth.token.email === 'urciknikolaj642@gmail.com' || root.child('roles/'+auth.uid+'/moderator').val() === true)",
        ".write": "auth != null"
      }
    },
    "profileChangeRequests": {
      ".read": "auth != null && auth.token.email === 'urciknikolaj642@gmail.com'",
      ".write": "auth != null",
      ".indexOn": [
        "ts"
      ],
      "$id": {
        ".validate": "newData.hasChildren(['uid','type','to','ts','status'])"
      }
    },
    "premiumRequests": {
      ".read": "auth != null && auth.token.email === 'urciknikolaj642@gmail.com'",
      ".write": "auth != null"
    },
    "bots": {
      ".read": "auth != null && auth.token.email === 'urciknikolaj642@gmail.com'",
      ".write": "auth != null && auth.token.email === 'urciknikolaj642@gmail.com'"
    },
    "botPosts": {
      ".read": true,
      "$id": {
        ".write": "auth != null && auth.token.email === 'urciknikolaj642@gmail.com'",
        ".validate": "(newData.child('ts').isNumber() || newData.child('at').isNumber()) && (newData.child('by').isString() || newData.child('from').isString())"
      }
    },
    "siteHelperConfig": {
      ".read": true,
      ".write": "auth != null && auth.token.email === 'urciknikolaj642@gmail.com'"
    },
    "settings": {
      ".read": true,
      ".write": "auth != null && auth.token.email === 'urciknikolaj642@gmail.com'"
    },
    "payments": {
      ".read": "auth != null && auth.token.email === 'urciknikolaj642@gmail.com'",
      ".write": "auth != null && auth.token.email === 'urciknikolaj642@gmail.com'",
      "requests": {
        "$uid": {
          ".read": "auth != null && (auth.uid === $uid || auth.token.email === 'urciknikolaj642@gmail.com')",
          ".write": "auth != null && auth.uid === $uid"
        }
      },
      "decisions": {
        "$uid": {
          ".read": "auth != null && (auth.uid === $uid || auth.token.email === 'urciknikolaj642@gmail.com')",
          ".write": "auth != null && auth.token.email === 'urciknikolaj642@gmail.com'"
        }
      }
    }
  }
}