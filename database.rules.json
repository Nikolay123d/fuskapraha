{
  "rules": {
    ".read": false,
    ".write": false,
    "roles": {
      "$uid": {
        ".read": "auth!=null && (auth.uid===$uid || auth.token.email==='urciknikolaj642@gmail.com')",
        ".write": "auth!=null && auth.token.email==='urciknikolaj642@gmail.com'"
      }
    },
    "bans": {
      "$uid": {
        ".read": "auth!=null && (auth.uid===$uid || auth.token.email==='urciknikolaj642@gmail.com')",
        ".write": "auth!=null && auth.token.email==='urciknikolaj642@gmail.com'",
        ".validate": "newData.hasChild('until') ? newData.child('until').isNumber() : true"
      }
    },
    "users": {
      "$uid": {
        ".read": "auth!=null && (auth.uid===$uid || auth.token.email==='urciknikolaj642@gmail.com')",
        ".write": "auth!=null && (auth.uid===$uid || auth.token.email==='urciknikolaj642@gmail.com')"
      }
    },
    "usersPublic": {
      "$uid": {
        ".read": true,
        ".write": "auth!=null && auth.uid===$uid"
      }
    },
    "presence": {
      "$uid": {
        ".read": true,
        ".write": "auth!=null && auth.uid===$uid"
      }
    },
    "messages": {
      ".read": true,
      "$city": {
        ".indexOn": [
          "ts"
        ],
        "$msg": {
          ".write": "auth!=null && !(root.child('bans/'+auth.uid+'/until').val() > now)",
          ".validate": "newData.hasChildren(['by','ts']) && newData.child('ts').isNumber()"
        }
      }
    },
    "chat": {
      ".read": true,
      "$city": {
        ".indexOn": [
          "at"
        ],
        "$msg": {
          ".write": "auth!=null && !(root.child('bans/'+auth.uid+'/until').val() > now)",
          ".validate": "newData.hasChildren(['from','at']) && newData.child('at').isNumber()"
        }
      }
    },
    "emails": {
      "$key": {
        ".read": "auth!=null && auth.token.email==='urciknikolaj642@gmail.com'",
        ".write": "auth!=null",
        ".validate": "newData.hasChildren(['uid','ts']) && newData.child('uid').isString() && newData.child('ts').isNumber()"
      }
    },
    "bots": {
      ".read": true,
      ".write": "auth!=null && auth.token.email==='urciknikolaj642@gmail.com'"
    },
    "privateMembers": {
      "$room": {
        "$uid": {
          ".read": "auth!=null && root.child('privateMembers/'+$room+'/'+auth.uid).val()===true",
          ".write": "auth!=null && (auth.uid===$uid || root.child('privateMembers/'+$room+'/'+auth.uid).val()===true) && !(root.child('bans/'+auth.uid+'/until').val() > now)",
          ".validate": "newData.isBoolean()"
        }
      }
    },
    "privateMessages": {
      "$room": {
        ".read": "auth!=null && root.child('privateMembers/'+$room+'/'+auth.uid).val()===true",
        ".write": "auth!=null && root.child('privateMembers/'+$room+'/'+auth.uid).val()===true && !(root.child('bans/'+auth.uid+'/until').val() > now)",
        ".indexOn": [
          "ts"
        ]
      }
    },
    "inboxMeta": {
      "$uid": {
        ".read": "auth!=null && auth.uid===$uid",
        ".write": "auth!=null && auth.uid===$uid",
        ".indexOn": [
          "lastTs",
          "ts"
        ]
      }
    },
    "friends": {
      "$uid": {
        ".read": "auth!=null && auth.uid===$uid",
        ".write": "auth!=null && auth.uid===$uid"
      }
    },
    "friendRequests": {
      "$uid": {
        ".read": "auth!=null && auth.uid===$uid",
        ".write": "auth!=null"
      }
    },
    "notifications": {
      "$uid": {
        ".read": "auth!=null && auth.uid===$uid",
        ".write": "auth!=null && auth.uid===$uid",
        ".indexOn": [
          "ts"
        ]
      }
    },
    "roomsBlocked": {
      "$room": {
        ".read": "auth!=null && auth.token.email==='urciknikolaj642@gmail.com'",
        ".write": "auth!=null && auth.token.email==='urciknikolaj642@gmail.com'",
        ".validate": "newData.isBoolean()"
      }
    },
    "rentMessages": {
      ".read": true,
      "$city": {
        ".indexOn": [
          "ts"
        ],
        "$msg": {
          ".write": "auth!=null && !(root.child('bans/'+auth.uid+'/until').val() > now)"
        }
      }
    },
    "ratings": {
      "$uid": {
        ".read": true,
        "$by": {
          ".write": "auth!=null && auth.uid===$by",
          ".validate": "newData.isNumber() && newData.val()>=1 && newData.val()<=5"
        }
      }
    },
    "profileChangeRequests": {
      ".read": "auth!=null && auth.token.email==='urciknikolaj642@gmail.com'",
      ".write": "auth!=null",
      ".indexOn": [
        "ts"
      ]
    },
    "payments": {
      "requests": {
        "$uid": {
          ".read": "auth!=null && (auth.uid===$uid || auth.token.email==='urciknikolaj642@gmail.com')",
          ".write": "auth!=null && auth.uid===$uid"
        }
      }
    }
  }
}