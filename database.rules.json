{
  "rules": {
    ".read": true,
    ".write": false,
    "messages": {
      "$city": {
        ".read": true,
        "$msg": {
          ".write": "auth != null && (!root.child('bans').child(auth.uid).exists() || root.child('bans').child(auth.uid).child('until').val() < now)"
        }
      }
    },
    "usersPublic": {
      "$uid": {
        ".read": true,
        ".write": "auth != null && auth.uid === $uid"
      }
    },
    "private": {
      "$tid": {
        ".read": "auth != null && $tid.matches('^'+auth.uid+'.*|.*'+auth.uid+'$')",
        "$msg": {
          ".write": "auth != null && ($tid.matches('^'+auth.uid+'.*') || $tid.matches('.*'+auth.uid+'$'))"
        }
      }
    },
    "board": {
      ".read": true,
      "$id": {
        ".write": "auth != null"
      }
    },
    "places": {
      ".read": true,
      "$id": {
        ".write": "auth != null"
      }
    },
    "reports": {
      ".read": "auth != null",
      "$id": {
        ".write": "auth != null"
      }
    },
    "payments": {
      "requests": {
        "$uid": {
          ".read": "auth != null && auth.uid===$uid",
          "$k": {
            ".write": "auth != null && auth.uid===$uid"
          }
        }
      },
      "decisions": {
        ".read": "auth != null && auth.token.email === 'urciknikolaj642@gmail.com'",
        "$uid": {
          ".write": "auth != null && auth.token.email === 'urciknikolaj642@gmail.com'"
        }
      }
    },
    "roles": {
      ".read": true,
      "$uid": {
        ".write": "auth != null && auth.token.email === 'urciknikolaj642@gmail.com'"
      }
    },
    "bans": {
      ".read": "auth != null && auth.token.email === 'urciknikolaj642@gmail.com'",
      "$uid": {
        ".write": "auth != null && auth.token.email === 'urciknikolaj642@gmail.com'"
      }
    },
    "settings": {
      ".read": true,
      "$k": {
        ".write": "auth != null && auth.token.email === 'urciknikolaj642@gmail.com'"
      }
    }
  }
}