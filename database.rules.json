{
  "rules": {
    ".read": true,
    "roles": {
      "$uid": {
        ".read": true,
        ".write": "auth != null"
      }
    },
    "bans": {
      "$uid": {
        ".read": true,
        ".write": "auth != null"
      }
    },
    "mutes": {
      "$uid": {
        ".read": true,
        ".write": "auth != null"
      }
    },
    "messages": {
      "$city": {
        ".indexOn": [
          "ts"
        ],
        "$msg": {
          ".read": true,
          ".write": "auth != null"
        }
      }
    },
    "rentMessages": {
      "$city": {
        ".indexOn": [
          "ts"
        ],
        "$msg": {
          ".read": true,
          ".write": "auth != null"
        }
      }
    },
    "presence": {
      "$uid": {
        ".read": true,
        ".write": "auth != null && auth.uid === $uid"
      }
    },
    "usersPublic": {
      "$uid": {
        ".read": true,
        ".write": "auth != null && auth.uid === $uid"
      }
    },
    "users": {
      "$uid": {
        ".read": "auth != null && auth.uid === $uid",
        ".write": "auth != null && auth.uid === $uid"
      }
    },
    "emails": {
      "$key": {
        ".read": "auth != null",
        ".write": "auth != null"
      }
    },
    "friendRequests": {
      "$toUid": {
        "$fromUid": {
          ".read": "auth != null && (auth.uid === $toUid || auth.uid === $fromUid)",
          ".write": "auth != null"
        }
      }
    },
    "friends": {
      "$uid": {
        "$friendUid": {
          ".read": "auth != null && (auth.uid === $uid || auth.uid === $friendUid)",
          ".write": "auth != null && auth.uid === $uid"
        }
      }
    },
    "inboxMeta": {
      ".read": "auth != null",
      ".write": "auth != null",
      ".indexOn": [
        "ts",
        "lastTs"
      ],
      "$uid": {
        ".read": "auth != null",
        ".write": "auth != null",
        ".indexOn": [
          "ts",
          "lastTs"
        ],
        "$peerUid": {
          ".read": "auth != null",
          ".write": "auth != null"
        }
      }
    },
    "privateMembers": {
      ".read": "auth != null",
      ".write": "auth != null",
      "$roomId": {
        ".read": "auth != null",
        ".write": "auth != null",
        "$uid": {
          ".read": "auth != null",
          ".write": "auth != null"
        }
      }
    },
    "privateMessages": {
      ".read": "auth != null",
      ".write": "auth != null",
      "$roomId": {
        ".read": "auth != null",
        ".write": "auth != null",
        ".indexOn": [
          "ts"
        ],
        "$msgId": {
          ".read": "auth != null",
          ".write": "auth != null"
        }
      }
    },
    "notifications": {
      "$uid": {
        ".read": "auth != null && auth.uid === $uid",
        ".write": "auth != null && auth.uid === $uid",
        ".indexOn": [
          "ts"
        ]
      }
    },
    "profileChangeRequests": {
      ".read": "auth != null",
      ".write": "auth != null",
      ".indexOn": [
        "ts"
      ]
    },
    "settings": {
      ".read": true,
      ".write": "auth != null"
    },
    "payments": {
      ".read": "auth != null",
      ".write": "auth != null"
    },
    "bots": {
      ".read": true,
      ".write": "auth != null"
    },
    "map": {
      "poi": {
        ".read": true,
        "$city": {
          "$id": {
            ".write": "auth != null"
          }
        }
      }
    }
  }
}