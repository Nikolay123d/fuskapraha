        }
      },
      "decisions": {
        "$uid": {
          ".read": "auth!=null && (auth.uid===$uid || auth.token.email === 'urciknikolaj642@gmail.com')",
          ".write": "auth!=null && auth.token.email === 'urciknikolaj642@gmail.com'"
        }
      },
      ".read": "auth!=null && auth.token.email === 'urciknikolaj642@gmail.com'",
      ".write": "auth!=null && auth.token.email === 'urciknikolaj642@gmail.com'"
    },
    "settings": {
      ".read": true,
      ".write": "auth!=null && auth.token.email === 'urciknikolaj642@gmail.com'"
    },
    "bots": {
      ".read": true,
      ".write": "auth!=null && auth.token.email === 'urciknikolaj642@gmail.com'"
    },
    "reports": {
      ".read": "auth!=null && (auth.token.email === 'urciknikolaj642@gmail.com' || root.child('roles/'+auth.uid+'/moderator').val()===true)",
