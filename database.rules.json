{
  "rules": {
    ".read": false,
    ".write": false,
    "admins": {
      ".read": "auth != null && root.child('admins/'+auth.uid).val() == true",
      "$uid": {
        ".write": "auth != null && root.child('admins/'+auth.uid).val() == true"
      }
    },
    "bans": {
      ".read": "auth != null && root.child('admins/'+auth.uid).val() == true",
      ".write": "auth != null && root.child('admins/'+auth.uid).val() == true"
    },
    "usersPublic": {
      "$uid": {
        ".read": true,
        ".write": "auth != null && auth.uid == $uid"
      }
    },
    "users": {
      "$uid": {
        ".read": "auth != null && auth.uid == $uid",
        ".write": "auth != null && auth.uid == $uid"
      }
    },
    "presence": {
      "$uid": {
        ".read": true,
        ".write": "auth != null && auth.uid == $uid"
      }
    },
    "notifications": {
      "$uid": {
        ".read": "auth != null && auth.uid == $uid",
        "$nid": {
          ".write": "auth != null && auth.uid == $uid",
          ".validate": "newData.hasChildren(['ts'])"
        },
        ".indexOn": [
          "ts"
        ]
      }
    },
    "friends": {
      "$uid": {
        ".read": "auth != null && auth.uid == $uid",
        "$friend": {
          ".write": "auth != null && (auth.uid == $uid || auth.uid == $friend)"
        }
      }
    },
    "friendRequests": {
      "$to": {
        ".read": "auth != null && auth.uid == $to",
        "$from": {
          ".read": "auth != null && (auth.uid == $to || auth.uid == $from)",
          ".write": "auth != null && (auth.uid == $to || auth.uid == $from)"
        }
      }
    },
    "inboxMeta": {
      "$uid": {
        ".read": "auth != null && auth.uid == $uid",
        ".write": "auth != null && auth.uid == $uid",
        ".indexOn": [
          "ts"
        ]
      }
    },
    "chat": {
      "$city": {
        ".read": true,
        "$msg": {
          ".write": "auth != null && root.child('bans/'+auth.uid).val() != true",
          ".validate": "newData.hasChildren(['uid','text','ts']) && newData.child('uid').val() == auth.uid"
        },
        ".indexOn": [
          "ts"
        ]
      }
    },
    "messages": {
      "$city": {
        ".read": true,
        "$msg": {
          ".write": "auth != null && root.child('bans/'+auth.uid).val() != true",
          ".validate": "newData.hasChildren(['uid','text','ts']) && newData.child('uid').val() == auth.uid"
        },
        ".indexOn": [
          "ts"
        ]
      }
    },
    "privateMembers": {
      "$room": {
        "$uid": {
          ".read": "auth != null && auth.uid == $uid",
          ".write": "auth != null && auth.uid == $uid"
        }
      }
    },
    "privateMessages": {
      "$room": {
        "$msg": {
          ".read": "auth != null && root.child('privateMembers/'+$room+'/'+auth.uid).val() == true",
          ".write": "auth != null && root.child('privateMembers/'+$room+'/'+auth.uid).val() == true",
          ".validate": "newData.hasChildren(['uid','text','ts']) && newData.child('uid').val() == auth.uid"
        },
        ".indexOn": [
          "ts"
        ]
      }
    },
    "bots": {
      ".read": true,
      ".write": "auth != null && root.child('admins/'+auth.uid).val() == true"
    },
    "botPosts": {
      ".read": true,
      ".write": "auth != null && root.child('admins/'+auth.uid).val() == true",
      ".indexOn": [
        "ts"
      ]
    },
    "vacancies": {
      ".read": true,
      "$vid": {
        ".write": "auth != null && (!data.exists() ? newData.child('owner').val() == auth.uid : data.child('owner').val() == auth.uid)"
      },
      ".indexOn": [
        "ts",
        "owner"
      ]
    },
    "profileChangeRequests": {
      ".read": "auth != null && root.child('admins/'+auth.uid).val() == true",
      "$rid": {
        ".write": "auth != null && auth.uid != null"
      },
      ".indexOn": [
        "ts"
      ]
    },
    "ratings": {
      ".read": true,
      "$uid": {
        "$rid": {
          ".write": "auth != null",
          ".validate": "newData.hasChildren(['from','value','ts']) && newData.child('from').val() == auth.uid"
        },
        ".indexOn": [
          "ts"
        ]
      }
    },
    "reportsUsers": {
      ".read": "auth != null && root.child('admins/'+auth.uid).val() == true",
      "$rid": {
        ".write": "auth != null && newData.child('from').val() == auth.uid"
      },
      ".indexOn": [
        "ts"
      ]
    },
    "payments": {
      "$uid": {
        ".read": "auth != null && auth.uid == $uid",
        ".write": "auth != null && auth.uid == $uid",
        ".indexOn": [
          "ts"
        ]
      }
    }
  }
}