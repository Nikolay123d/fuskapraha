{
  "rules": {
    "messages": {
      ".read": true,
      "$city": {
        ".indexOn": [
          "ts"
        ],
        "$msg": {
          ".write": "auth!=null && !root.child('banned').child(auth.uid).exists()",
          ".validate": "newData.hasChildren(['by','ts']) && newData.child('ts').isNumber() && (newData.child('text').isString() || newData.child('photo').isString())"
        }
      }
