{
  "rules": {
    "messages": {
      ".read": true,
      ".write": "auth!=null",
      "$city": {
        ".indexOn": [
          "ts"
        ],
        "$msg": {
          ".validate": "newData.hasChildren(['by','ts']) && newData.child('ts').isNumber() && ((newData.child('text').isString() && newData.child('text').val().length <= 2000) || newData.child('photo').isString() || (newData.child('text').val()===null && newData.child('photo').val()!==null))"
        }
      }
    },
    "rentMessages": {
      ".read": true,
      ".write": "auth!=null",
      "$city": {
        ".indexOn": [
          "ts"
        ]
      }
    },
    "help": {
      ".read": true,
      "$city": {
        "cards": {
          ".read": true,
          ".write": "auth!=null && auth.token.email === 'urciknikolaj642@gmail.com'"
        }
      }
    },
    "map": {
