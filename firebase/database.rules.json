{
  "rules": {
    ".read": true,
    "messages": {
      "$city": {
        ".read": true,
        ".write": "auth != null"
      }
    },
    "rentMessages": {
      "$city": {
        ".read": true,
        ".write": "auth != null"
      }
    },
    "dm": {
      "$key": {
        ".read": "auth != null && auth.token != null",
        ".write": "auth != null"
      }
    },
    "friends": {
      "$uid": {
        ".read": "auth != null && auth.uid === $uid",
        ".write": "auth != null && auth.uid === $uid"
      }
    },
    "usersPublic": {
      "$uid": {
        ".read": true,
        ".write": "auth != null && auth.uid === $uid"
      }
    },
    "map": {
      "poi": {
        "$city": {
          ".read": true,
          ".write": "root.child('admins').child(auth.uid).val() === true"
        }
      }
    },
    "help": {
      "$city": {
        ".read": true,
        ".write": "auth != null"
      }
    },
    "settings": {
      ".read": true,
      ".write": "root.child('admins').child(auth.uid).val() === true"
    },
    "admins": {
      ".read": false,
      ".write": false
    }
  }
}