here{
  "rules": {
    ".read": true,
    ".write": false,

    "users": {
      "$uid": { ".read": true, ".write": "auth != null && auth.uid === $uid" }
    },
    "presence": {
      "$uid": { ".read": true, ".write": "auth != null && auth.uid === $uid" }
    },

    "messages": {
      ".read": true,
      "$mid": { ".write": "auth != null && newData.child('uid').val() === auth.uid" }
    },
    "rentMessages": {
      ".read": true,
      "$mid": { ".write": "auth != null && newData.child('uid').val() === auth.uid" }
    },

    "privateMessages": {
      ".read": "auth != null",
      "$cid": {
        "$mid": { ".write": "auth != null && newData.child('from').val() === auth.uid" }
      }
    },

    "friendRequests": {
      ".read": "auth != null",
      "$toUid": { "$fromUid": { ".write": "auth != null && $fromUid === auth.uid" } }
    },
    "friends": {
      ".read": "auth != null",
      "$uid": { "$other": { ".write": "auth != null && ($uid === auth.uid || $other === auth.uid)" } }
    },

    "notifications": {
      "$uid": {
        ".read": "auth != null && auth.uid === $uid",
        "$nid": { ".write": "auth != null && auth.uid === $uid" }
      }
    },
    "inboxMeta": {
      "$uid": {
        ".read": "auth != null && auth.uid === $uid",
        "$other": { ".write": "auth != null && auth.uid === $uid" }
      }
    },

    "payments": {
      "inbox": {
        "$uid": {
          ".read": "auth != null && (auth.uid === $uid || root.child('settings/admins/' + auth.uid).val() === true || auth.token.email === 'urciknikolaj642@gmail.com' || auth.token.email === 'darausoan@gmail.com')",
          "$pid": { ".write": "auth != null && auth.uid === $uid" }
        }
      }
    },

    "settings": {
      "admins": {
        ".read": "auth != null",
        ".write": "auth != null && (root.child('settings/admins/' + auth.uid).val() === true || auth.token.email === 'urciknikolaj642@gmail.com' || auth.token.email === 'darausoan@gmail.com')"
      },
      "theme": {
        ".read": true,
        ".write": "auth != null && (root.child('settings/admins/' + auth.uid).val() === true || auth.token.email === 'urciknikolaj642@gmail.com' || auth.token.email === 'darausoan@gmail.com')"
      },
      "userFlags": {
        "$uid": {
          ".read": "auth != null",
          ".write": "auth != null && (auth.uid === $uid || root.child('settings/admins/' + auth.uid).val() === true || auth.token.email === 'urciknikolaj642@gmail.com' || auth.token.email === 'darausoan@gmail.com')"
        }
      },
      "bots": {
        ".read": "auth != null",
        ".write": "auth != null && (root.child('settings/admins/' + auth.uid).val() === true || auth.token.email === 'urciknikolaj642@gmail.com' || auth.token.email === 'darausoan@gmail.com')"
      }
    }
  }
}
