{
  "rules": {
    ".read": false,
    ".write": false,

    "roles": {
      "$uid": {
        ".read": "auth != null && (auth.token.email === 'urciknikolaj642@gmail.com' || auth.token.email === 'urciknikolaj62@gmail.com')",
        ".write": "auth != null && (auth.token.email === 'urciknikolaj642@gmail.com' || auth.token.email === 'urciknikolaj62@gmail.com')"
      }
    },

    "bans": {
      "$uid": {
        ".read": "auth != null && (auth.uid === $uid || auth.token.email === 'urciknikolaj642@gmail.com' || auth.token.email === 'urciknikolaj62@gmail.com')",
        ".write": "auth != null && (auth.token.email === 'urciknikolaj642@gmail.com' || auth.token.email === 'urciknikolaj62@gmail.com')",
        ".validate": "newData.hasChild('until') && newData.child('until').isNumber()"
      }
    },

    "mutes": {
      "$uid": {
        ".read": "auth != null && (auth.uid === $uid || auth.token.email === 'urciknikolaj642@gmail.com' || auth.token.email === 'urciknikolaj62@gmail.com')",
        ".write": "auth != null && (auth.token.email === 'urciknikolaj642@gmail.com' || auth.token.email === 'urciknikolaj62@gmail.com')",
        ".validate": "newData.hasChild('until') && newData.child('until').isNumber()"
      }
    },

    "usersPublic": {
      "$uid": {
        ".read": true,
        ".write": "auth != null && (auth.uid === $uid || auth.token.email === 'urciknikolaj642@gmail.com' || auth.token.email === 'urciknikolaj62@gmail.com')"
      }
    },

    "users": {
      "$uid": {
        ".read": "auth != null && auth.uid === $uid",
        ".write": "auth != null && auth.uid === $uid"
      }
    },

    "presence": {
      ".indexOn": ["ts"],
      "$uid": {
        ".read": true,
        ".write": "auth != null && auth.uid === $uid",
        ".validate": "newData.hasChild('ts') && newData.child('ts').isNumber()"
      }
    },

    "messages": {
      "$city": {
        ".indexOn": ["ts"],
        "$msg": {
          ".read": true,
          ".write": "auth != null && (root.child('bans/'+auth.uid+'/until').val() === null || root.child('bans/'+auth.uid+'/until').val() <= now) && (root.child('mutes/'+auth.uid+'/until').val() === null || root.child('mutes/'+auth.uid+'/until').val() <= now)",
          ".validate": "newData.hasChild('ts') && newData.child('ts').isNumber()"
        }
      }
    },

    "rentMessages": {
      "$city": {
        ".indexOn": ["ts"],
        "$msg": {
          ".read": true,
          ".write": "auth != null && (root.child('bans/'+auth.uid+'/until').val() === null || root.child('bans/'+auth.uid+'/until').val() <= now) && (root.child('mutes/'+auth.uid+'/until').val() === null || root.child('mutes/'+auth.uid+'/until').val() <= now)",
          ".validate": "newData.hasChild('ts') && newData.child('ts').isNumber()"
        }
      }
    },

    "friendRequests": {
      "$to": {
        "$from": {
          ".read": "auth != null && (auth.uid === $to || auth.uid === $from)",
          ".write": "auth != null && auth.uid === $from && (root.child('bans/'+auth.uid+'/until').val() === null || root.child('bans/'+auth.uid+'/until').val() <= now)",
          ".validate": "newData.hasChild('ts') && newData.child('ts').isNumber()"
        }
      }
    },

    "friends": {
      "$a": {
        "$b": {
          ".read": "auth != null && (auth.uid === $a || auth.uid === $b)",
          ".write": "auth != null && (auth.uid === $a || auth.uid === $b)"
        }
      }
    },

    "privateMembers": {
      "$room": {
        ".read": "auth != null && data.child(auth.uid).val() === true",
        ".write": "auth != null && (root.child('bans/'+auth.uid+'/until').val() === null || root.child('bans/'+auth.uid+'/until').val() <= now) && (root.child('mutes/'+auth.uid+'/until').val() === null || root.child('mutes/'+auth.uid+'/until').val() <= now)",
        "$uid": {
          ".write": "auth != null && (auth.uid === $uid || data.child(auth.uid).val() === true || !data.exists())",
          ".validate": "newData.isBoolean()"
        }
      }
    },

    "privateMessages": {
      "$room": {
        ".indexOn": ["ts"],
        ".read": "auth != null && root.child('privateMembers/'+$room+'/'+auth.uid).val() === true",
        ".write": "auth != null && (root.child('bans/'+auth.uid+'/until').val() === null || root.child('bans/'+auth.uid+'/until').val() <= now) && (root.child('mutes/'+auth.uid+'/until').val() === null || root.child('mutes/'+auth.uid+'/until').val() <= now) && root.child('privateMembers/'+$room+'/'+auth.uid).val() === true",
        "$msg": {
          ".validate": "newData.hasChild('ts') && newData.child('ts').isNumber()"
        }
      }
    },

    "privateRoomsByUser": {
      "$uid": {
        ".read": "auth != null && auth.uid === $uid",
        ".write": "auth != null && auth.uid === $uid",
        "$room": {
          ".validate": "newData.isBoolean()"
        }
      }
    },

    "inboxMeta": {
      "$uid": {
        ".indexOn": ["lastTs", "ts"],
        ".read": "auth != null && auth.uid === $uid",
        ".write": "auth != null && auth.uid === $uid",
        "$chatId": {
          ".validate": "newData.hasChild('lastTs') && newData.child('lastTs').isNumber()"
        }
      }
    },

    "notifications": {
      "$uid": {
        ".indexOn": ["ts"],
        ".read": "auth != null && auth.uid === $uid",
        ".write": "auth != null && auth.uid === $uid",
        "$nid": {
          ".validate": "newData.hasChildren(['ts','type','read']) && newData.child('ts').isNumber() && newData.child('type').isString() && newData.child('read').isBoolean()"
        }
      }
    },

    "payments": {
      "requests": {
        ".read": "auth != null && (auth.token.email === 'urciknikolaj642@gmail.com' || auth.token.email === 'urciknikolaj62@gmail.com')",
        "$uid": {
          ".read": "auth != null && (auth.uid === $uid || auth.token.email === 'urciknikolaj642@gmail.com' || auth.token.email === 'urciknikolaj62@gmail.com')",
          ".write": "auth != null && (auth.uid === $uid || auth.token.email === 'urciknikolaj642@gmail.com' || auth.token.email === 'urciknikolaj62@gmail.com')"
        }
      },
      "audit": {
        "$uid": {
          ".read": "auth != null && (auth.uid === $uid || auth.token.email === 'urciknikolaj642@gmail.com' || auth.token.email === 'urciknikolaj62@gmail.com')",
          ".write": "auth != null && (auth.token.email === 'urciknikolaj642@gmail.com' || auth.token.email === 'urciknikolaj62@gmail.com')"
        }
      }
    },

    "announcements": {
      "global": {
        ".read": true,
        ".write": "auth != null && (auth.token.email === 'urciknikolaj642@gmail.com' || auth.token.email === 'urciknikolaj62@gmail.com')",
        ".validate": "newData.hasChild('ts') && newData.child('ts').isNumber()"
      }
    },

    "settings": {
      ".read": true,
      ".write": "auth != null && (auth.token.email === 'urciknikolaj642@gmail.com' || auth.token.email === 'urciknikolaj62@gmail.com')"
    },

    "map": {
      "poi": {
        "$city": {
          ".read": true,
          ".write": "auth != null && ((auth.token.email === 'urciknikolaj642@gmail.com' || auth.token.email === 'urciknikolaj62@gmail.com') || root.child('roles/'+auth.uid+'/moderator').val() === true)"
        }
      }
    },

    "support": {
      "tickets": {
        ".read": "auth != null && (auth.token.email === 'urciknikolaj642@gmail.com' || auth.token.email === 'urciknikolaj62@gmail.com')",
        "$id": {
          ".write": "auth != null",
          ".validate": "newData.hasChild('ts') && newData.child('ts').isNumber()"
        }
      }
    }
  }
}
